{
    "$schema": "./manifest_schema.json",
    "manifest_version": 4,
    "type": "framework",
    "name": "Socket.io support In QQNT",
    "slug": "socket_qqnt",
    "description": "在 LiteLoaderQQNT 中使用 socket.io (Client)",
    "version": "1.0.0",
    "icon": "./icon.png",
    "thumb": "./icon.svg",
    "authors": [
        {
            "name": "Chumenta",
            "link": "https://github.com/QWXL"
        }
    ],
    "platform": [
        "win32",
        "linux",
        "darwin"
    ],
    "injects": {
        "renderer": "./src/renderer.js",
        "main": "./src/main.js",
        "preload": "./src/preload.js"
    },
    "repository": {
        "repo": "QWXL/LiteLoaderQQNT-socket.io",
        "branch": "main",
        "release": {
            "tag": "1.0.0",
            "file": "LiteLoaderQQNT-socket.io.zip"
        }
    }
}